source 'https://rubygems.org'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'github-pages', versions['github-pages']
gem 'jekyll-feed'
gem 'jekyll-paginate'
gem 'jekyll-mentions'
gem 'jekyll-coffeescript'
gem 'jekyll-redirect-from'
gem 'rouge'

gem 'html-proofer'
